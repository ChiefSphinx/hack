{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#BAA4C7",
          "style": "plain",
          "template": "\u250f[<#ffffff>ü§†</>{{ .UserName }} from <#ffffff>üíª</>{{ .HostName }}]",
          "type": "session"
        },        
        {
          "foreground": "#CDB4DB",
          "style": "plain",
          "template": "[<#ffffff>‚õÖ</>{{ .Name }}]",
          "type": "az"
        },
        {
          "foreground": "#CDB4DB",
          "style": "plain",
          "template": "[<#ffffff>üê≥</>{{.Context}}{{if .Namespace}} :: {{.Namespace}}{{end}}]",
          "type": "kubectl"
        },
        {
          "foreground": "#FFAFCC",
          "properties": {
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true
          },
          "style": "plain",
          "template": "[<#ffffff>{{ .UpstreamIcon }}</>{{ .HEAD }}{{ .BranchStatus }}{{ if .Working.Changed }} <#ffffff>\uf044</> {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} <#ffffff>\uf046</> {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} <#ffffff>\uf692</> {{ .StashCount }}{{ end }}]",
          "type": "git"
        },
        {
          "foreground": "#FFAFCC",
          "style": "plain",
          "template": "[\uf0e7]",
          "type": "root"
        },
        {
          "foreground": "#DEC8E5",
          "style": "powerline",
          "template": "[<#ffffff>üêõ</>Error, check your command]",
          "type": "exit"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#BDE0FE",
          "properties": {
            "style": "dallas",
            "threshold": 0
          },
          "style": "diamond",
          "template": "[<#ffffff>‚åõ</>{{ .FormattedMs }}s]",
          "type": "executiontime"
        },
        {
          "properties": {
            "root_icon": "\uf292"
          },
          "style": "diamond",
          "template": "\uf0e7",
          "type": "root"
        },
        {
          "foreground": "#A2D2FF",
          "style": "diamond",
          "template": "[<#ffffff>‚ö°</>CPU:{{ round .PhysicalPercentUsed .Precision }}% RAM:{{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1000000000.0) }}/{{ (div .PhysicalTotalMemory 1000000000.0) }}GB]",
          "type": "sysinfo"
        },
        {
          "foreground": "#A2D2FF",
          "properties": {
            "time_format": "Monday at 3:04:05 PM"
          },
          "style": "diamond",
          "template": "[<#ffffff>üïí</>{{ .CurrentDate | date .Format }}]",
          "type": "time"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#CDB4DB",
          "properties": {
            "style": "full"
          },
          "style": "plain",
          "template": "\u2523[üìç<#AAD6FF>{{ .Path }}</>]",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#CDB4DB",
          "style": "plain",
          "template": "\u2517[üóø",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": false,
  "version": 2
}
